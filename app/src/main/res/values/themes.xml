<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Car_match" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryVariant">@color/little_blue</item>
        <item name="colorOnPrimary">@color/white</item>
<!--Secondary brand-->
        <item name="colorSecondary">@color/Green</item>
        <item name="colorSecondaryVariant">@color/Dark_blue</item>
        <item name="colorOnSecondary">@color/blue</item>
<!--        statusbar-->
        <item name="android:statusBarColor">?attr/colorOnSecondary</item>



    </style>
    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/silde_in</item>
        <item name="android:windowExitAnimation">@anim/slide_out</item>

    </style>

    <style name="Theme.Car_match" parent="Base.Theme.Car_match" />
</resources>